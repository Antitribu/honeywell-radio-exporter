{
  "dashboard": {
    "id": null,
    "title": "RAMSES RF System Monitor",
    "tags": [
      "ramses",
      "rf",
      "hvac",
      "home-automation"
    ],
    "style": "dark",
    "timezone": "browser",
    "templating": {
      "list": [
        {
          "name": "zone",
          "type": "query",
          "label": "Zone",
          "datasource": null,
          "query": {
            "query": "label_values(ramses_device_setpoint_celsius, zone_name)",
            "refId": "PrometheusVariableQueryEditor-VariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "multi": true,
          "includeAll": true,
          "allValue": null,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "sort": 1,
          "skipUrlSync": false
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Message Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ramses_messages_total[5m])",
            "legendFormat": "{{verb}} {{code}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "lines": true,
        "points": false,
        "linewidth": 2
      },
      {
        "id": 2,
        "title": "Active Devices",
        "type": "stat",
        "targets": [
          {
            "expr": "ramses_active_devices",
            "legendFormat": "Active Devices"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 16,
        "title": "Zone Window Status",
        "type": "stat",
        "targets": [
          {
            "expr": "ramses_zone_window_open",
            "legendFormat": "{{zone_name}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "Closed",
                    "color": "green"
                  },
                  "1": {
                    "text": "Open",
                    "color": "blue"
                  }
                }
              }
            ]
          }
        },
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 18,
        "title": "Device Last Seen",
        "type": "table",
        "targets": [
          {
            "expr": "time() - ramses_device_last_seen_timestamp",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "ramses_device_setpoint_celsius * 0 + 1",
            "format": "table",
            "instant": true,
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "job": true,
                "instance": true,
                "monitor": true,
                "origin_prometheus": true,
                "Value #B": true,
                "zone_idx": true,
                "device_role": true
              },
              "indexByName": {
                "device_id": 0,
                "device_name": 1,
                "zone_name": 2,
                "Value #A": 3
              },
              "renameByName": {
                "device_id": "Device ID",
                "device_name": "Device Name",
                "zone_name": "Zone",
                "Value #A": "Seconds Since Last Seen"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background",
              "align": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Seconds Since Last Seen"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {
                        "value": 0,
                        "color": "green"
                      },
                      {
                        "value": 300,
                        "color": "yellow"
                      },
                      {
                        "value": 600,
                        "color": "red"
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 4,
        "title": "Top Message Types",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (code, verb) (rate(ramses_message_types_total[5m])))",
            "format": "table"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "job": true,
                "instance": true,
                "monitor": true,
                "origin_prometheus": true
              }
            }
          }
        ]
      },
      {
        "id": 5,
        "title": "Device Communications",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (source_device, destination_device) (rate(ramses_device_communications_total[5m]))",
            "format": "heatmap"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ramses_message_errors_total[5m])",
            "legendFormat": "{{error_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "lines": true,
        "points": false,
        "linewidth": 2
      },
      {
        "id": 100,
        "title": "Zone: $zone",
        "type": "row",
        "collapsed": false,
        "repeat": "zone",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 101,
        "title": "$zone - Temperature & Setpoint",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(ramses_device_temperature_celsius{zone_name=~\"$zone\"})",
            "legendFormat": "Actual Temperature",
            "refId": "A"
          },
          {
            "expr": "avg(ramses_device_setpoint_celsius{zone_name=~\"$zone\"})",
            "legendFormat": "Setpoint",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 33
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "spanNulls": true,
              "showPoints": "never"
            },
            "unit": "celsius",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Actual Temperature"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "blue"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Setpoint"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                },
                {
                  "id": "custom.lineStyle",
                  "value": {
                    "dash": [
                      10,
                      10
                    ],
                    "fill": "dash"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "table",
            "placement": "bottom",
            "calcs": [
              "last",
              "mean",
              "min",
              "max"
            ]
          }
        }
      },
      {
        "id": 102,
        "title": "$zone - Heat Demand",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(ramses_heat_demand{zone_idx!=\"FC\", zone_name=~\"$zone\"}) * 100",
            "legendFormat": "Heat Demand",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 33
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "spanNulls": true,
              "showPoints": "never"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "blue"
                },
                {
                  "value": 20,
                  "color": "green"
                },
                {
                  "value": 50,
                  "color": "yellow"
                },
                {
                  "value": 75,
                  "color": "orange"
                },
                {
                  "value": 90,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "table",
            "placement": "bottom",
            "calcs": [
              "last",
              "mean",
              "max"
            ]
          }
        }
      },
      {
        "id": 103,
        "title": "$zone - Message Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(ramses_messages_total{zone_name=~\"$zone\"}[5m]))",
            "legendFormat": "Messages per second",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 41
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "spanNulls": true,
              "showPoints": "never"
            },
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "table",
            "placement": "bottom",
            "calcs": [
              "last",
              "mean",
              "max"
            ]
          }
        }
      },
      {
        "id": 25,
        "title": "Temperature Related",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 50
        }
      },
      {
        "id": 9,
        "title": "Heat Demand by Zone",
        "type": "graph",
        "targets": [
          {
            "expr": "ramses_heat_demand{zone_idx!=\"FC\"} * 100",
            "legendFormat": "{{zone_name}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 51
        },
        "yaxes": [
          {
            "format": "percent",
            "label": "Heat Demand",
            "min": 0,
            "max": 100
          }
        ],
        "lines": true,
        "points": false,
        "linewidth": 2
      },
      {
        "id": 10,
        "title": "Boiler Communication Health",
        "type": "stat",
        "targets": [
          {
            "expr": "time() - ramses_boiler_last_seen_timestamp",
            "legendFormat": "{{boiler_id}} ({{boiler_name}})"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 12,
          "y": 51
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 120,
                  "color": "yellow"
                },
                {
                  "value": 300,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 11,
        "title": "Boiler Message Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ramses_boiler_messages_sent_total[5m]) * 60",
            "legendFormat": "Sent to {{boiler_id}}"
          },
          {
            "expr": "rate(ramses_boiler_messages_received_total[5m]) * 60",
            "legendFormat": "Received from {{boiler_id}}"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 59
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Messages/min"
          }
        ],
        "lines": true,
        "points": false,
        "linewidth": 2
      },
      {
        "id": 12,
        "title": "Boiler Response Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "sum by (boiler_id, boiler_name) (ramses_boiler_messages_received_total) / sum by (boiler_id, boiler_name) (ramses_boiler_messages_sent_total)",
            "legendFormat": "{{boiler_id}}"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 59
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 0.5,
                  "color": "yellow"
                },
                {
                  "value": 0.8,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Boiler Message Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (message_type) (ramses_boiler_messages_sent_total)",
            "legendFormat": "{{message_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 59
        }
      },
      {
        "id": 14,
        "title": "Boiler Message History",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (boiler_id, boiler_name, message_code, message_type) (ramses_boiler_messages_sent_total)",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 67
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "job": true,
                "instance": true,
                "monitor": true,
                "origin_prometheus": true
              },
              "indexByName": {
                "boiler_id": 0,
                "boiler_name": 1,
                "message_code": 2,
                "message_type": 3,
                "Value": 4
              },
              "renameByName": {
                "boiler_id": "Boiler ID",
                "boiler_name": "Name",
                "message_code": "Code",
                "message_type": "Type",
                "Value": "Count"
              }
            }
          }
        ]
      },
      {
        "id": 17,
        "title": "System Sync Status",
        "type": "gauge",
        "targets": [
          {
            "expr": "ramses_system_sync_remaining_seconds",
            "legendFormat": "{{device_id}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 75
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "max": 156,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 30,
                  "color": "yellow"
                },
                {
                  "value": 60,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 19,
        "title": "Zone Modes",
        "type": "table",
        "targets": [
          {
            "expr": "ramses_zone_mode_info{zone_idx=~\".+\"} == 1",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 83
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "Value": true,
                "device_id": true,
                "zone_idx": true,
                "job": true,
                "instance": true,
                "monitor": true,
                "origin_prometheus": true,
                "device_role": true
              },
              "indexByName": {
                "zone_name": 0,
                "mode": 1
              },
              "renameByName": {
                "zone_name": "Zone",
                "mode": "Active Mode"
              }
            }
          }
        ]
      },
      {
        "id": 21,
        "title": "Boiler Setpoint",
        "type": "stat",
        "targets": [
          {
            "expr": "ramses_boiler_setpoint_celsius",
            "legendFormat": "{{boiler_name}} ({{boiler_id}})"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 91
        },
        "fieldConfig": {
          "defaults": {
            "unit": "celsius",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "blue",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 40
                },
                {
                  "color": "orange",
                  "value": 60
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          }
        }
      },
      {
        "id": 22,
        "title": "Boiler Modulation Level",
        "type": "gauge",
        "targets": [
          {
            "expr": "ramses_boiler_modulation_level * 100",
            "legendFormat": "{{boiler_name}}"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 91
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "blue",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 20
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "orange",
                  "value": 75
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true,
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          }
        }
      },
      {
        "id": 23,
        "title": "Boiler Status",
        "type": "stat",
        "targets": [
          {
            "expr": "ramses_boiler_flame_active",
            "legendFormat": "Flame"
          },
          {
            "expr": "ramses_boiler_ch_active",
            "legendFormat": "Central Heating"
          },
          {
            "expr": "ramses_boiler_dhw_active",
            "legendFormat": "Hot Water"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 91
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "OFF",
                    "color": "semi-dark-blue"
                  },
                  "1": {
                    "text": "ON",
                    "color": "semi-dark-orange"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "semi-dark-blue",
                  "value": null
                },
                {
                  "color": "semi-dark-orange",
                  "value": 1
                }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          }
        }
      },
      {
        "id": 24,
        "title": "Boiler Setpoint & Modulation History",
        "type": "graph",
        "targets": [
          {
            "expr": "ramses_boiler_setpoint_celsius",
            "legendFormat": "Setpoint - {{boiler_name}}",
            "yAxisIndex": 0
          },
          {
            "expr": "ramses_boiler_modulation_level * 100",
            "legendFormat": "Modulation - {{boiler_name}}",
            "yAxisIndex": 1
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 91
        },
        "yaxes": [
          {
            "format": "celsius",
            "label": "Setpoint (\u00b0C)",
            "show": true
          },
          {
            "format": "percent",
            "label": "Modulation (%)",
            "show": true,
            "min": 0,
            "max": 100
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "legend": {
          "show": true,
          "alignAsTable": false,
          "avg": false,
          "current": true,
          "max": false,
          "min": false,
          "rightSide": false,
          "total": false,
          "values": true
        },
        "lines": true,
        "points": false,
        "linewidth": 2
      },
      {
        "id": 201,
        "type": "row",
